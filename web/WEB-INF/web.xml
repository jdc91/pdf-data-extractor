<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <context-param>
        <param-name>MONGODB_HOST</param-name>
        <param-value>localhost</param-value>
    </context-param>
    <context-param>
        <param-name>MONGODB_PORT</param-name>
        <param-value>27017</param-value>
    </context-param>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>TemplateUploadController</servlet-name>
        <servlet-class>com.data.extractor.controllers.TemplateUploadController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkUpTemplateRegionController</servlet-name>
        <servlet-class>com.data.extractor.controllers.MarkUpTemplateRegionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExtractPdfController</servlet-name>
        <servlet-class>com.data.extractor.controllers.ExtractPdfController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormPopulateController</servlet-name>
        <servlet-class>com.data.extractor.controllers.FormPopulateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageCategoriesController</servlet-name>
        <servlet-class>com.data.extractor.controllers.ManageCategoriesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InitController</servlet-name>
        <servlet-class>com.data.extractor.controllers.InitController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NavigationController</servlet-name>
        <servlet-class>com.data.extractor.controllers.NavigationController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditTemplateController</servlet-name>
        <servlet-class>com.data.extractor.controllers.EditTemplateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DictionaryController</servlet-name>
        <servlet-class>com.data.extractor.controllers.DictionaryController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditMarkupController</servlet-name>
        <servlet-class>com.data.extractor.controllers.EditMarkupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExtractEditTempController</servlet-name>
        <servlet-class>com.data.extractor.controllers.ExtractEditTempController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PatternControllerServlet</servlet-name>
        <servlet-class>com.data.extractor.controllers.PatternControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageUsersController</servlet-name>
        <servlet-class>com.data.extractor.controllers.ManageUsersController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccessRightController</servlet-name>
        <servlet-class>com.data.extractor.controllers.AccessRightController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionController</servlet-name>
        <servlet-class>com.data.extractor.controllers.SessionController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TemplateUploadController</servlet-name>
        <url-pattern>/TemplateUploadController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkUpTemplateRegionController</servlet-name>
        <url-pattern>/MarkUpTemplateRegionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExtractPdfController</servlet-name>
        <url-pattern>/ExtractPdfController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormPopulateController</servlet-name>
        <url-pattern>/FormPopulateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageCategoriesController</servlet-name>
        <url-pattern>/ManageCategoriesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InitController</servlet-name>
        <url-pattern>/InitController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NavigationController</servlet-name>
        <url-pattern>/NavigationController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditTemplateController</servlet-name>
        <url-pattern>/EditTemplateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DictionaryController</servlet-name>
        <url-pattern>/DictionaryController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditMarkupController</servlet-name>
        <url-pattern>/EditMarkupController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExtractEditTempController</servlet-name>
        <url-pattern>/ExtractEditTempController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PatternControllerServlet</servlet-name>
        <url-pattern>/PatternControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageUsersController</servlet-name>
        <url-pattern>/ManageUsersController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccessRightController</servlet-name>
        <url-pattern>/AccessRightController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionController</servlet-name>
        <url-pattern>/SessionController</url-pattern>
    </servlet-mapping>


</web-app>